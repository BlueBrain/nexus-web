{
  "name": "nexus-fusion",
  "description": "The interface of Blue Brain Nexus, the open-source knowledge graph for data-driven science.",
  "author": "Blue Brain project",
  "license": "Apache-2.0",
  "repository": "github:bluebrain/nexus-explorer",
  "url": "https://github.com/BlueBrain/nexus/issues/new?labels=fusion",
  "type": "module",
  "engine": {
    "node": "20.10.0",
    "npm": ">=9.8.1",
    "yarn": "1.22.x"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev:client": "yarn clean && concurrently 'vite build -c ./vite.config.refresh.js' 'vite'",
    "dev:server": "NODE_ENV=development tsx --tsconfig ./tsconfig.node.json ./server/index.ts",
    "dev": "concurrently 'yarn dev:client' 'yarn dev:server'",
    "build:server": "etsc --config=etsc.config.cjs",
    "build:client": "vite build -c ./vite.config.refresh.js && vite build",
    "build": "yarn clean && yarn build:client && yarn build:server",
    "preview": "cross-env NODE_ENV=production node ./dist/index.mjs",
    "test:watch": "vitest watch",
    "test:ci": "vitest run",
    "test:ci:silent": "vitest run --silent",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "knip": "knip",
    "lint": "tslint --project tsconfig.json",
    "style": "prettier --check \"./src/**/*.{ts,tsx,js,jsx,less}\"",
    "style:fix": "prettier --write \"./src/**/*.{ts,tsx,js,jsx,less}\"",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run --config-file cypress.config.ts",
    "e2e:ci": "echo | sudo docker exec --user=$UID -t cypress cypress run --headless --config-file cypress.config.ts --browser chrome",
    "e2e:ps": "echo | cypress run --config-file cypress-public-studios.config.ts --browser chrome --spec \"cypress/e2e/PublicStudios.cy.ts\"",
    "e2e:ui": "API_ENDPOINT=http://test start-server-and-test dev http://localhost:8000 cy:run"
  },
